<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:fitsSystemWindows="true"
              android:orientation="vertical">

    <include layout="@layout/layout_toolbar"/>

    <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/colorLine"/>

    <TextView
            android:id="@+id/countTime"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="12dp"
            android:background="#eff5f8"
            android:gravity="center"
            android:text="统计时间："
            android:textSize="12sp"/>

    <!--<androidx.core.widget.NestedScrollView-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:overScrollMode="never">-->
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">


            <LinearLayout

                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:background="@drawable/shape_bonus_ly"
                    android:layout_height="70dp">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="35dp"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="总业绩:"/>
                <TextView
                        android:id="@+id/totalMoney"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_marginLeft="5dp"
                        android:textColor="@color/colorBlackText"
                        android:text="100"/>

            </LinearLayout>

            <RelativeLayout

                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:background="@drawable/shape_bonus_ly"
                    android:layout_height="70dp">
                <TextView
                        android:id="@+id/team"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="35dp"
                        android:layout_centerVertical="true"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="团队总人数:"/>
                <TextView
                        android:id="@+id/teamTotal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/team"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="5dp"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="0"/>
                <TextView
                        android:id="@+id/refresh"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="35dp"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:textSize="14sp"
                        android:textColor="@color/whit"
                        android:paddingLeft="10dp"
                        android:paddingTop="5dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="5dp"
                        android:background="@drawable/shape_distribute_fresh"
                        android:text="更新"/>
            </RelativeLayout>
            <LinearLayout

                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:background="@drawable/shape_bonus_ly"
                    android:layout_height="70dp">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="35dp"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="一级总人数:"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="接口无"/>

            </LinearLayout>
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="5dp"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="35dp"
                    android:background="@drawable/shape_bonus_ly">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="上级ID:"/>
                <TextView
                        android:id="@+id/superId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:layout_marginLeft="5dp"
                        android:text="无"/>
            </LinearLayout>
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="5dp"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingLeft="35dp"
                    android:background="@drawable/shape_bonus_ly">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="上级名称:"/>
                <TextView
                        android:id="@+id/superName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textSize="16sp"
                        android:textColor="@color/colorBlackText"
                        android:text="无上级"/>
            </LinearLayout>

            <LinearLayout

                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:background="@drawable/shape_bonus_ly"
                    android:layout_height="wrap_content"
                    android:paddingTop="20dp"
                    android:paddingBottom="20dp"
                    android:orientation="vertical">

                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:id="@+id/teamDetail"
                            android:layout_width="76dp"
                            android:layout_height="76dp"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:layout_weight="1"
                            android:layout_marginRight="12dp"
                            android:layout_marginLeft="12dp"
                            android:background="#f5f5f5">
                        <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/shangpin"/>
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp"
                                android:textSize="12sp"
                                android:textColor="@color/colorBlackText"
                                android:text="业绩明细"/>


                    </LinearLayout>
                    <LinearLayout
                            android:id="@+id/detailRecord"
                            android:layout_width="76dp"
                            android:layout_height="76dp"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:background="#f5f5f5"
                            android:layout_weight="1">
                        <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/huiyuan"/>
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp"
                                android:textSize="12sp"
                                android:textColor="@color/colorBlackText"
                                android:text="明细记录"/>


                    </LinearLayout>
                    <LinearLayout
                            android:id="@+id/teamList"
                            android:layout_width="76dp"
                            android:layout_height="76dp"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:background="#f5f5f5"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:layout_marginLeft="12dp">
                        <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:src="@drawable/guanli"/>
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp"
                                android:textSize="12sp"
                                android:textColor="@color/colorBlackText"
                                android:text="团队列表"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_gravity="center_horizontal"
                        android:orientation="horizontal">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:textColor="@color/colorBlackText"
                            android:text="我的ID:"/>
                    <TextView
                            android:id="@+id/myId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:textSize="16sp"
                            android:textColor="@color/colorBlackText"
                            android:text="17959909"/>

                </LinearLayout>


            </LinearLayout>


        </LinearLayout>

